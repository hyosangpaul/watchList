{
  "manifest_version" : 3,
  "name" : "WatchList",
  "description": "Simply save the list you want to watch",
  "version": "1.0.0",
  "icons": {
    "16" : "images/logo16.png",
    "32" : "images/logo32.png",
    "48" : "images/logo48.png",
    "128" : "images/logo128.png"
  },
  "action": {
    "default_icon": "images/logo32.png",
    "default_title": "WatchList",
    "default_popup": "popup/popup.html",
    "default_js" : "js/common.js"
  },
  "permissions": ["storage"]
}